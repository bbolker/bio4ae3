	pmid = {28356449},
	pages = {20162562},
	file = {Full Text PDF:/home/bolker/Documents/zotero_new/storage/4PLRQAAF/Kennedy and Read - 2017 - Why does drug resistance readily evolve but vaccin.pdf:application/pdf;Snapshot:/home/bolker/Documents/zotero_new/storage/4F7C6CQX/20162562.html:text/html},
}

