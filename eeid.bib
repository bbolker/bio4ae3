}

@article{cunningham_quantifying_2021,
	title = {Quantifying 25 years of disease-caused declines in {Tasmanian} devil populations: host density drives spatial pathogen spread},
	volume = {24},
	issn = {1461-0248},
	shorttitle = {Quantifying 25 years of disease-caused declines in {Tasmanian} devil populations},
