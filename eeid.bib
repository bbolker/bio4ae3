	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13621},
	keywords = {Chytridiomycosis, compensatory recruitment, density-dependent transmission, environmental refugia, host‚Äìpathogen, management, population persistence, population recovery, resistance, tolerance},
	pages = {130--148},
	file = {Full Text PDF:/home/bolker/Documents/zotero_new/storage/RBEXCXR3/Brannelly et al. - 2021 - Mechanisms underlying host persistence following a.pdf:application/pdf},
}

