}

@article{mcclelland_major_2004,
	title = {Major histocompatibility complex controls the trajectory but not host-speciÔ¨Åc adaptation during virulence evolution of the pathogenic fungus {Cryptococcus} neoformans},
	volume = {271},
	abstract = {Genes of the major histocompatibility complex (MHC) play a critical role in immune recognition and are the most genetically diverse loci known. One hypothesis to explain this diversity postulates that pathogens adapt to common MHC haplotypes and thus favour selection of new or rare alleles. To determine whether the pathogenic yeast Cryptococcus neoformans adapts to MHC-dependent immune responses, it was serially passaged in two independent replicate lines of Ô¨Åve B10 MHC-congenic strains and Balb/c mice. All passaged lines increased in virulence as measured by reduced host survival. MHC inÔ¨Çuenced the rate (trajectory) of virulence increase during passages as measured by signiÔ¨Åcant diÔ¨Äerences in mortality rate (p {\textless} 0.001). However, when the post-passage strains were tested, no MHC diÔ¨Äerences in mortality rate remained and only minor diÔ¨Äerences in titres were observed. Also contrary to expectations, increased virulence in three lines passaged in B 10 mice had a larger eÔ¨Äect in Balb/c mice, and the evolution of virulence in lines passaged in alternating hosts was not retarded. To our knowledge, these data represent the Ô¨Årst experimental test of MHC-speciÔ¨Åc adaptation in a non-viral pathogen. The failure to observe MHC eÔ¨Äects despite dramatically increased virulence and host-genotype-speciÔ¨Åc adaptation to non-MHC genes suggests that escape of MHC-dependent immune recognition may be diÔ¨Äicult for pathogens with unlimited epitopes or that other virulence factors can swamp MHC eÔ¨Äects.},
	number = {1548},
	journal = {Proceedings of the Royal Society of London Series B-Biological Sciences},
	author = {McClelland, E. E and Adler, F. R and Granger, D. L and Potts, W. K},
	year = {2004},
	keywords = {CLASS-I;, REVERSE-TRANSCRIPTASE;, HEPATITIS-B-VIRUS;, MICE, IMMUNODEFICIENCY-VIRUS;, parasite, VARIANTS;, T-LYMPHOCYTE, TRADE-OFF;, SELECTION;, ADVANTAGE;, RESPONSES;, HETEROZYGOTE},
	pages = {1557--1564},
}

