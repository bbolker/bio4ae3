	note = {Publisher: Royal Society},
	keywords = {anthrax, disease dynamics, ecological trap, environmental transmission, habitat selection, transmission hotspot},
	pages = {20210582},
	file = {Full Text PDF:/home/bolker/Documents/zotero_new/storage/IR99IICE/Huang et al. - 2021 - Disease or drought environmental fluctuations rel.pdf:application/pdf},
