@article{west_sex_2000,
	title = {Sex allocation and population structure in apicomplexan (protozoa) parasites},
	volume = {267},
	abstract = {Establishing the selÔ¨Ång rate of parasites is important for studies in clinical and epidemiological medicine as well as evolutionary biology. Sex allocation theory oÔ¨Äers a relatively cheap and easy way to estimate selÔ¨Ång rates in natural parasite populations. Local mate competition (LMC) theory predicts that the optimal sex ratio (r*; deÔ¨Åned as proportion males) is related to the selÔ¨Ång rate (s) by the equation r*=(1-s)/2. In this paper, we generalize the application of sex allocation theory across parasitic protozoa in the phylum Apicomplexa. This cosmopolitan phylum consists entirely of parasites, and includes a number of species of medical and veterinary importance. We suggest that LMC theory should apply to eimeriorin intestinal parasites. As predicted, data from 13 eimeriorin species showed a female-biased sex ratio, with the sex ratios suggesting high levels of selÔ¨Ång (0.8-1.0). Importantly our estimate of the selÔ¨Ång rate in one of these species, Toxoplasma gondii, is in agreement with previous genetic analyses. In contrast, we predict that LMC theory will not apply to the groups in which syzygy occurs (adeleorins, gregarines and piroplasms). Syzygy occurs when a single male gametocyte and a single female gametocyte pair together physically or in close proximity, just prior to fertilization. As predicted, data from four adeleorin species showed sex ratios not signiÔ¨Åcantly diÔ¨Äerent from 0.5.},
	number = {1440},
	journal = {Proceedings of the Royal Society of London Series B-Biological Sciences},
	author = {West, S. A and Smith, T. G and Read, A. F},
	year = {2000},
	keywords = {PLASMODIUM;, LOCAL, MALARIA;, LIFE-CYCLES;, EVOLUTION;, TOXOPLASMA-GONDII;, parasite, MATING, Ratio;, MATE, VIRULENCE, COMPETITION;, PHYLOGENETIC, ANALYSIS;, PATTERNS;},
	pages = {257--263},
}

@article{reece_sex_2003,
	title = {Sex ratios in the rodent malaria parasite, {Plasmodium} chabaudi},
	volume = {127},
	abstract = {The sex ratios of malaria and related Apicomplexan parasites play a major role in transmission success. Here, we address 2 fundamental issues in the sex ratios of the rodent malaria parasite, Plasmodium chabaudi. First we test the accuracy of empirical methods for estimating sex ratios in malaria parasites, and show that sex ratios made with standard thin smears may overestimate the proportion of female gametocytes. Secondly, we test whether the mortality rate diÔ¨Äers between male and female gametocytes, as assumed by sex ratio theory. Conventional application of sex ratio theory to malaria parasites assumes that the primary sex ratio can be accurately determined from mature gametocytes circulating in the peripheral circulation. We stopped gametocyte production with chloroquine in order to study a cohort of gametocytes in vitro. The mortality rate was signiÔ¨Åcantly higher for female gametocytes, with an average half-life of 8 h for female gametocytes and 16 h for male gametocytes.},
	journal = {Parasitology},
	author = {Reece, S. E and Duncan, A. B and West, S. A and Read, A. F},
	year = {2003},
	keywords = {FERTILITY;, MOSQUITOS;, eeidclass, GAMETOCYTES;, COMMITMENT;, parasite, TRANSMISSION;, FALCIPARUM, BERGHEI;, INFECTIVITY;, DEVELOPMENT;, FERTILIZATION;, VECTOR, GAMETE},
